#lang:tamil


int முதல் = 0;
int இரண்டு = 1;
int மூன்று = 1;

int முறை = 10;

print ("%d முறை பிபோனச்சி எண்கள்: \n", முறை+2);
print ("%d ", முதல்);
print ("%d ", இரண்டு);
for (int ஐ = 0; ஐ < முறை; ஐ++){
    மூன்று = முதல் + இரண்டு;
    print ("%d ", மூன்று);
    முதல் = இரண்டு;
    இரண்டு = மூன்று;
}
print ("\n");